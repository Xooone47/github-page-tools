{
  "name": "fexpy",
  "version": "0.0.1",
  "description": "To build a react app quickly",
  "scripts": {
    "start": "cross-env webpack-dev-server --mode development",
    "clean": "rm -rf build/",
    "build": "sh ./scripts/build.sh",
    "test": "jest --config ./configs/jest.config.js",
    "test:watch": "jest --config ./configs/jest.config.js --watch",
    "lint": "yarn lint-es && yarn lint-type",
    "lint-es": "eslint src --ext .tsx,.ts,.js,.jsx -c .eslintrc.js --ignore-path ./configs/.eslintignore",
    "lint-type": "tsc",
    "lint:report": "yarn lint-es -f node_modules/eslint-rich-reporter/reporter.js -o eslint-report.html",
    "generateAnalyzeFile": "mkdir -p ./build/static && webpack --mode production --profile --json > ./build/static/stats.json",
    "analyze": "yarn generateAnalyzeFile && webpack-bundle-analyzer --port 8847 ./build/static/stats.json"
  },
  "bin": {
    "fe": "bin/index.js"
  },
  "babel": {
    "extends": "./configs/babel.config.js"
  },
  "author": "Deland Li",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.0.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.9.0",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-jest": "28.1.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-import": "^1.6.6",
    "babel-plugin-react-require": "^3.0.0",
    "babel-plugin-react-transform": "^3.0.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^7.0.3",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^5.2.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^7.11.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-rich-reporter": "^0.0.10",
    "eslint-webpack-plugin": "^3.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.3.5",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^6.7.0",
    "react-refresh": "^0.11.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^0.20.2",
    "thread-loader": "^3.0.4",
    "ts-jest": "^24.3.0",
    "webpack": "^5.65.0",
    "webpack-build-notifier": "^2.0.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.14.0",
    "ahooks": "^3.7.0",
    "antd": "^4.18.9",
    "axios": "^0.21.1",
    "classnames": "^2.2.5",
    "commander": "^5.1.0",
    "copy-to-clipboard": "^3.3.1",
    "immer": "^9.0.15",
    "lodash": "^4.17.19",
    "lodash-decorators": "^4.5.0",
    "moment": "^2.29.1",
    "prop-types": "^15.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "recoil": "^0.7.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.4.2",
    "use-immer": "^0.7.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint -c .eslintrc.js --ignore-path ./configs/.eslintignore --fix",
      "git add ."
    ]
  },
  "browserslist": [
    "defaults"
  ]
}
